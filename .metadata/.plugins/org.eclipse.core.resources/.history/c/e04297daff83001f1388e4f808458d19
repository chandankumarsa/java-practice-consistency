import java.util.Scanner;

public class SUM_OF_DIGIT {
	public static void main(String[] args) {
		System.out.println("ENter a number to perform the SUM OF DIGIT");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum =0;
		while(n>0) {
			int rem =n%10;
			sum= sum+rem;
		}
		n = n/10;
		
	}

}
